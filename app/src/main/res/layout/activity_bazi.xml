                <TextView
                    android:id="@+id/tv_wangshuai_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="死" />

                <TextView
                    android:id="@+id/tv_wangshuai_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="衰" />

                <TextView
                    android:id="@+id/tv_wangshuai_4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="病" />

                <TextView
                    android:id="@+id/tv_wangshuai_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="帝旺" />

                <TextView
                    android:id="@+id/tv_wangshuai_6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="临官" />

                <TextView
                    android:id="@+id/tv_wangshuai_7"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="冠带" />

            </LinearLayout>
            <!--纳音-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/tv_nayin_0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_6"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

                <TextView
                    android:id="@+id/tv_nayin_7"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:paddingBottom="5dp"
                    android:text="[金箔金]"
                    android:textSize="10dp" />

            </LinearLayout>
            <!--十神-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/tv_shishen_0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="食神"
                    android:textColor="@color/mention" />

                <TextView
                    android:id="@+id/tv_shishen_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="劫财"
                    android:textColor="@color/mention" />

                <TextView
                    android:id="@+id/tv_shishen_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="比肩"
                    android:textColor="@color/mention" />

                <TextView
                    android:id="@+id/tv_shishen_3"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="正印"
                    android:textColor="@color/mention" />

                <TextView
                    android:id="@+id/tv_shishen_4"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="枭神"
                    android:textColor="@color/mention" />

                <TextView
                    android:id="@+id/tv_shishen_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="正官"
                    android:textColor="@color/mention" />

                <TextView
                    android:id="@+id/tv_shishen_6"
