<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorRed"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <ScrollView
        android:layout_margin="10dp"
        android:id="@+id/scroll_bazi"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="5dp">

            <!--生日-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:text="阳历:" />

                <TextView
                    android:id="@+id/tv_Birth_yang"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="20"
                    android:text="1900年12月12日"
                    android:textColor="@color/dark" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:text="阴历:"

                    />

                <TextView
                    android:id="@+id/tv_Birth_yin"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="20"
                    android:text="一九零零年正月廿一"
                    android:textColor="@color/dark" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:text="生肖:" />

                <TextView
                    android:id="@+id/tv_animal"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:text="猫"
                    android:textColor="@color/dark" />
            </LinearLayout>
            <!--八字-->
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_sex_bazi_up"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toStartOf="@+id/tv_yearshen_bazi"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/tv_sex_bazi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="乾造:"
                    app:layout_constraintBottom_toTopOf="@+id/tv_sex_bazi_down"
                    app:layout_constraintEnd_toStartOf="@+id/tv_year_gan"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_sex_bazi_up" />

                <TextView
                    android:id="@+id/tv_sex_bazi_down"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/tv_year_zhi"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/tv_yearshen_bazi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="劫财"
                    android:textColor="@color/mention"
                    app:layout_constraintEnd_toStartOf="@+id/tv_monthshen_bazi"
                    app:layout_constraintStart_toEndOf="@+id/tv_sex_bazi_up" />

                <TextView
                    android:id="@+id/tv_monthshen_bazi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="偏财"
                    android:textColor="@color/mention"
                    app:layout_constraintEnd_toStartOf="@+id/tv_dayshen"
                    app:layout_constraintStart_toEndOf="@+id/tv_yearshen_bazi" />

                <TextView
                    android:id="@+id/tv_dayshen"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="日主"
                    android:textColor="@color/mention"
                    app:layout_constraintEnd_toStartOf="@+id/tv_hourshen_bazi"
                    app:layout_constraintStart_toEndOf="@+id/tv_monthshen_bazi" />

                <TextView
                    android:id="@+id/tv_hourshen_bazi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="食神"
                    android:textColor="@color/mention"
                    app:layout_constraintEnd_toStartOf="@+id/tv_kongwu_bazi_up"
                    app:layout_constraintStart_toEndOf="@+id/tv_dayshen" />

                <TextView
                    android:id="@+id/tv_year_gan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="辛"
                    app:layout_constraintEnd_toStartOf="@+id/tv_month_gan"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_sex_bazi"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yearshen_bazi" />

                <TextView
                    android:id="@+id/tv_month_gan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="庚"
                    app:layout_constraintEnd_toStartOf="@+id/tv_day_gan"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_year_gan"
                    app:layout_constraintTop_toBottomOf="@+id/tv_yearshen_bazi" />

                <TextView
                    android:id="@+id/tv_day_gan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="甲"
                    app:layout_constraintEnd_toStartOf="@+id/tv_hour_gan"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_month_gan"
                    app:layout_constraintTop_toBottomOf="@+id/tv_monthshen_bazi" />

                <TextView
                    android:id="@+id/tv_hour_gan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="癸"
                    app:layout_constraintEnd_toStartOf="@+id/tv_kongwu_bazi"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_day_gan"
                    app:layout_constraintTop_toBottomOf="@+id/tv_dayshen" />


                <TextView
                    android:id="@+id/tv_year_zhi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="卯"
                    app:layout_constraintEnd_toStartOf="@+id/tv_month_zhi"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_sex_bazi_down"
                    app:layout_constraintTop_toBottomOf="@+id/tv_year_gan" />

                <TextView
                    android:id="@+id/tv_month_zhi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="子"
                    app:layout_constraintEnd_toStartOf="@+id/tv_day_zhi"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_year_zhi"
                    app:layout_constraintTop_toBottomOf="@+id/tv_month_gan" />

                <TextView
                    android:id="@+id/tv_day_zhi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="午"
                    app:layout_constraintEnd_toStartOf="@+id/tv_hour_zhi"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_month_zhi"
                    app:layout_constraintTop_toBottomOf="@+id/tv_month_gan" />

                <TextView
                    android:id="@+id/tv_hour_zhi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="酉"
                    app:layout_constraintEnd_toStartOf="@+id/tv_kongwu_down"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_day_zhi"
                    app:layout_constraintTop_toBottomOf="@+id/tv_day_gan" />

                <TextView
                    android:id="@+id/tv_kongwu_bazi_up"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/tv_hourshen_bazi"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tv_kongwu_bazi"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="子丑空"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_hour_gan"
                    app:layout_constraintTop_toBottomOf="@+id/tv_kongwu_bazi_up" />

                <TextView
                    android:id="@+id/tv_kongwu_down"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/tv_hour_zhi" />
            </android.support.constraint.ConstraintLayout>
            <!--五行纳音-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />

                <TextView
                    android:id="@+id/tv_wuxingnayin_year"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="[剑锋金]"
                    android:textColor="@color/dark" />

                <TextView
                    android:id="@+id/tv_wuxingnayin_month"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="[剑锋金]"
                    android:textColor="@color/dark" />

                <TextView
                    android:id="@+id/tv_wuxingnayin_day"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="[剑锋金]"
                    android:textColor="@color/dark" />

                <TextView
                    android:id="@+id/tv_wuxingnayin_hour"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="[剑锋金]"
                    android:textColor="@color/dark" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>
            <!--藏干-->
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/t0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="藏干:"
                    app:layout_constraintEnd_toStartOf="@+id/ll_year_zanggan"
                    app:layout_constraintStart_toStartOf="parent" />
                <!--地支藏干，代码中动态添加-->

                <LinearLayout
                    android:id="@+id/ll_year_zanggan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toStartOf="@+id/ll_month_zanggan"
                    app:layout_constraintStart_toEndOf="@+id/t0">

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_month_zanggan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toStartOf="@+id/ll_day_zanggan"
                    app:layout_constraintStart_toEndOf="@+id/ll_year_zanggan">

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_day_zanggan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toStartOf="@+id/ll_hour_zanggan"
                    app:layout_constraintStart_toEndOf="@+id/ll_month_zanggan">

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_hour_zanggan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toStartOf="@+id/t5"
                    app:layout_constraintStart_toEndOf="@+id/ll_day_zanggan">

                </LinearLayout>

                <TextView
                    android:id="@+id/t5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/ll_hour_zanggan" />
            </android.support.constraint.ConstraintLayout>
            <!--神煞-->
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_shen_0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/ll_year_shensha"
                    android:text="神煞:" />
                <!--神煞，代码中动态添加-->

                <LinearLayout
                    android:id="@+id/ll_year_shensha"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:layout_constraintStart_toEndOf="@+id/tv_shen_0"
                    app:layout_constraintEnd_toStartOf="@+id/ll_month_shensha"
                    android:orientation="vertical">

                </LinearLayout>

                <LinearLayout
                    app:layout_constraintStart_toEndOf="@+id/ll_year_shensha"
                    app:layout_constraintEnd_toStartOf="@+id/ll_day_shenshea"
                    android:id="@+id/ll_month_shensha"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                </LinearLayout>

                <LinearLayout
                    app:layout_constraintStart_toEndOf="@+id/ll_month_shensha"
                    app:layout_constraintEnd_toStartOf="@+id/ll_hour_shensha"
                    android:id="@+id/ll_day_shenshea"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                </LinearLayout>

                <LinearLayout
                    app:layout_constraintEnd_toStartOf="@+id/shen_5"
                    app:layout_constraintStart_toEndOf="@+id/ll_day_shenshea"
                    android:id="@+id/ll_hour_shensha"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                </LinearLayout>
                <!---->

                <TextView
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/ll_hour_shensha"
                    android:id="@+id/shen_5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </android.support.constraint.ConstraintLayout>
            <!--旺衰-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="旺衰:" />

                <TextView
                    android:id="@+id/tv_wangshuai_0"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="绝" />

                <TextView
                    android:id="@+id/tv_wangshuai_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
